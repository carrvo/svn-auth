#!/bin/sh
set -e

#DEBHELPER#

if true; then
	if [ -e /usr/share/apache2/apache2-maintscript-helper ] ; then
		. /usr/share/apache2/apache2-maintscript-helper
		# do not want these to be enabled by default!
		for conf in user-override  ; do
			apache2_invoke disconf $conf  || exit 1
		done
	fi
fi

cd /usr/src/svn-auth/
chgrp www-data svn-auth.php
chmod g+x svn-auth.php

exit 0
